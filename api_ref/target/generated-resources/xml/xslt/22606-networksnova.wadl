         <method name="POST" rax:id="createNetworks">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Create Networks">
               <para role="shortdesc">Creates a network for the specified tenant ID.</para>
               <para>This operation creates a network for the tenant ID specified in the request URI.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample>
                        <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                           <programlisting language="javascript">{
    "network": 
        {
            "cidr": "192.168.0.0/24", 
            "label": "superprivate"
        }
}</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
                  <param name="." style="plain" path="$.parameters " rax:id=""/>
                  <param name="cidr"
                         style="plain"
                         type="capi:UUID"
                         required="true"
                         path="$.parameters.cidr"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook">
                        <para>The IP block from which to allocate the network. For example, <emphasis role="bold">172.16.0.0/24</emphasis> or <emphasis role="bold">2001:DB8::/64</emphasis>. For more
                            information about CIDR notation, see
                            <link xlink:href="http://www.rackspace.com/knowledge_center/article/using-cidr-notation">
                              <citetitle>
                                Using CIDR Notation in CLoud Networks</citetitle>
                           </link>
                            in the KNowledge Center.</para>
                    </wadl:doc>
                  </param>
                  <param name="label"
                         style="plain"
                         type="capi:UUID"
                         required="true"
                         path="$.parameters.label"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook">
                        <para>The name of the new network. For example, <emphasis role="bold">my_new_network</emphasis>.</para>
                    </wadl:doc>
                  </param>
               </representation>
               <representation mediaType="application/xml">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample>
                        <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                           <programlisting language="xml">&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;network 
  cidr="192.168.0.0/24" 
  label="superprivate_xml"
/&gt; 
</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample>
                        <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                           <programlisting language="javascript">{
    "network": {
        "cidr": "192.168.0.0/24", 
        "id": "1ff4489e-db0e-45a6-8c9f-4616c6ef5db1", 
        "label": "superprivate"
    }
}</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
                  <param name="." style="plain" path="$.parameters " rax:id=""/>
                  <param name="id"
                         style="plain"
                         type="xsd:string"
                         path="$.parameters.id"
                         rax:id="">
                    <wadl:doc xmlns="http://www.w3.org/1999/xhtml" xml:lang="EN" title="id">
