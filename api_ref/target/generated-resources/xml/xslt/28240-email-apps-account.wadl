               <param name="referenceNumber"
                      style="plain"
                      type="xsd:string"
                      required="false"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The reference number for the
                  account</para>
                  </wadl:doc>
               </param>
               <representation mediaType="application/json" element="noop">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <xsdxt:code>
                        <programlisting language="javascript">PUT '/customers/999999', 'text/xml', { 'name'
                  =\u003E 'NewName', 'referenceNumber' =\u003E '12345'
