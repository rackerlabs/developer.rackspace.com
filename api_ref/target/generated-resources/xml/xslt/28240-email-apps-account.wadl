            <resource path="customers/{customer_account_number}/loginToken" id="logintoken">
         <param name="customer_account_number"
                style="template"
                required="true"
                rax:id="">
               <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
