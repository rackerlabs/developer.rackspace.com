        <resource path="/v2/{tenant_id}/flavors/{flavor_id}/os-extra_specs"
                id="os-extra-specs">
         <param name="tenant_id" style="template" type="xsd:string" rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>The ID for the tenant or
                            account in a multi-tenancy
                        cloud.</para>
            </wadl:doc>
                </param>
         <param name="flavor_id" style="template" required="true" rax:id="">
