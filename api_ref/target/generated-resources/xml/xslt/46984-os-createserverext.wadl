                      title="Create server with configuration drive">
               <para role="shortdesc">Creates a server with the
                configuration drive extended attribute.</para>
            </wadl:doc>
            <request>
            
               <param name="security_groups"
                      style="plain"
                      required="false"
                      type="xsd:string"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>A <code>security_groups</code>
                        object.</para>
                  </wadl:doc>
               </param>
               <param name="security_group"
                      style="plain"
                      required="false"
                      type="xsd:string"
