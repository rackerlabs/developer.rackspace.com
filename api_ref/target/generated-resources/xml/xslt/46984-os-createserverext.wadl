               </param>
               <param name="name"
                      style="plain"
                      required="true"
                      type="csapi:string"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="name">
                    <para>The server name.</para>
                  </wadl:doc>
               </param>
               <param name="metadata"
                      style="plain"
                      required="false"
                      type="csapi:string"
